	function BindUnion(UpazilaCode) {
		$.ajax({
			type: "GET",
			url: "/Common/GetUnion",
			data: { UpazilaCode: UpazilaCode },
			contentType: "application/json; charset=utf-8",
			processData: true,
			success: function (data) {
				$("#ddlUnionCode").select2("val", "");
				$("#ddlUnionCode").empty();
				$("#ddlUnionCode").append($("<option />").val("").text("Select a Union"));
				$.each(data, function () {
					$("#ddlUnionCode").append($("<option />").val(this.Value).text(this.Text));
				});
			}, error: function (jqXHR, exception) {
				var errorMsg = AjaxErrorHandle(jqXHR, exception);
				Notification('Error', errorMsg, 'danger');
			}
		});
	}
</script>

