@using Resources
@using ESAQAApp.Models
@model ESAQAApp.Models.Master.Master
@{
	ViewBag.Title = "New | Khana Survey (Baseline) Data Entry";
}

<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="~/Scripts/js/vaccord/easy-responsive-tabs.css" rel="stylesheet" />
<script src="~/Scripts/js/vaccord/easyResponsiveTabs.js"></script>

<section class="panel panel-default">
	<div class="row wrapper">
        <div class="col-sm-8 m-b-xs">
            <h3 class="bold" style="margin: 0 !important;"><b>খানা জরিপ প্রশ্নপত্র</b> - (বেইজলাইন ২০১৯-২০২০) [কেস নংঃ <span id="spnSavedCaseNo">-</span>]</h3>
        </div>
		<div class="col-sm-4">
			<div class="btn-group pull-right right">
				<a class="btn btn-default" href="@Url.Action("Index")">
					<i class="fa fa-table"></i> @Resources.GoToList
				</a>
			</div>
		</div>
	</div>
	<header class="panel-heading bg-light" style="padding: 10px 15px 28px;">
        <ul class="nav nav-tabs pull-left">
			
            <li class="active">
                <a href="#Master" data-toggle="tab"><i class="fa fa-pencil"></i> প্রধান ফর্ম</a>
            </li>
            <li class="">
                <a href="#TabTwo" data-toggle="tab"><i class="fa fa-pencil"></i> মডিউল ১ নংঃ আর্থ-সামাজিক অবস্থা</a>
            </li>
            <li class="">
                <a href="#TabThree" data-toggle="tab"><i class="fa fa-pencil"></i> মডিউল ২ নংঃ কৃষি সংক্রান্ত</a>
            </li>
            <li class="">
                <a href="#TabFour" data-toggle="tab"><i class="fa fa-pencil"></i> মডিউল ৩ নংঃ পানি ব্যবস্থাপনা সংক্রান্ত</a>
            </li>
            <li class="">
                <a href="#TabFive" data-toggle="tab"><i class="fa fa-pencil"></i> মডিউল ৪ নংঃ পরিবেশ সংক্রান্ত</a>
            </li>
            <li class="">
                <a href="#TabSix" data-toggle="tab"><i class="fa fa-pencil"></i> মডিউল ৫ নংঃ প্রাতিষ্ঠানিক</a>
            </li>

            
        </ul>
	</header>
	<div class="panel-body">
		<div class="tab-content">
			<div class="tab-pane fade active in" id="Master">
				<div class="col-sm-12">
					<div class="panel panel-info">
						<div class="panel-heading">
							প্রধান ফর্ম
						</div>
						<div class="panel-body">
							<div class="row">
								<input id="MasterDataId" type="hidden" value="1" />

								<div class="col-md-12">
									<div class="form-group col-md-4">
										@Html.LabelFor(model => model.CaseNo, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.CaseNo, new { htmlAttributes = new { @class = "form-control text-center", @readonly = "readonly" } })
										@Html.ValidationMessageFor(model => model.CaseNo, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-4">
										@Html.LabelFor(model => model.FormDate, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.FormDate, new { htmlAttributes = new { @class = "form-control datepicker-input text-center" } })
										@Html.ValidationMessageFor(model => model.FormDate, "", new { @class = "text-danger" })
									</div>

									<div class="form-group col-md-8 pull-right">
										@Html.LabelFor(model => model.PlaceOfInterview, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.PlaceOfInterview, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.PlaceOfInterview, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-6">
										<fieldset>
											<legend style="font-size: 100%;">জিপিএস কো-অর্ডিনেট (অক্ষাংশ-Latitude)</legend>

											<div class="col-md-12" style="padding: 0 0 0 0;">
												<div class="col-md-4" style="padding-left: 0;">
													<div class="input-group m-b">
														<span class="input-group-addon bold">Deg</span>
														<input id="tab1LatDeg" type="number" onblur="CalcLatLongToDD(1)" class="form-control text-right" min="0" max="2" />
													</div>
												</div>
												<div class="col-md-4">
													<div class="input-group m-b">
														<span class="input-group-addon bold">Min</span>
														<input id="tab1LatMin" type="number" onblur="CalcLatLongToDD(1)" class="form-control text-right" min="0" max="2" />
													</div>
												</div>
												<div class="col-md-4" style="padding-right: 0;">
													<div class="input-group m-b">
														<span class="input-group-addon bold">Sec</span>
														<input id="tab1LatSec" type="number" onblur="CalcLatLongToDD(1)" class="form-control text-right" min="0" max="5" />
													</div>
												</div>
											</div>
										</fieldset>

										<div class="input-group m-b">
											<span class="input-group-addon bold">Decimal Point</span>
											@Html.EditorFor(model => model.LocLatDecimal, new { htmlAttributes = new { @type = "number", @min = "0", @class = "form-control text-right" } })
										</div>
									</div>

									<div class="form-group col-md-6">
										<fieldset>
											<legend style="font-size: 100%;">জিপিএস কো-অর্ডিনেট (দ্রাঘিমাংশ-Longitude)</legend>

											<div class="col-md-12" style="padding: 0 0 0 0;">
												<div class="col-md-4" style="padding-left: 0;">
													<div class="input-group m-b">
														<span class="input-group-addon bold">Deg</span>
														<input id="tab1LongDeg" type="number" onblur="CalcLatLongToDD(2)" class="form-control text-right" min="0" max="2" />
													</div>
												</div>
												<div class="col-md-4">
													<div class="input-group m-b">
														<span class="input-group-addon bold">Min</span>
														<input id="tab1LongMin" type="number" onblur="CalcLatLongToDD(2)" class="form-control text-right" min="0" max="2" />
													</div>
												</div>
												<div class="col-md-4" style="padding-right: 0;">
													<div class="input-group m-b">
														<span class="input-group-addon bold">Sec</span>
														<input id="tab1LongSec" type="number" onblur="CalcLatLongToDD(2)" class="form-control text-right" min="0" max="5" />
													</div>
												</div>
											</div>
										</fieldset>

										<div class="input-group m-b">
											<span class="input-group-addon bold">Decimal Point</span>
											@Html.EditorFor(model => model.LocLongDecimal, new { htmlAttributes = new { @type = "number", @min = "0", @class = "form-control text-right" } })
										</div>
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-12">
										<fieldset>
											<legend><span> সুবিধাভোগী - উত্তরদাতার ধরণ (টিক চিহ্ন দিন)</span></legend>

											<div class="form-group col-md-2">
												<div class="checkbox m-r-xs">
													<label>
														<input id="chkBenificiary1" name="beniandnonbeni" class="grpBeneficiary beniandnonbeni" style="width: 20px; height: 20px;" type="checkbox" value="1">
														<span style="padding-left: 10px; font-size: 20px;"> বাধ নির্মাণ </span>
													</label>
												</div>
											</div>

											<div class="form-group col-md-2">
												<div class="checkbox m-l-xs m-r-xs">
													<label>
														<input id="chkBenificiary2" name="beniandnonbeni" class="grpBeneficiary beniandnonbeni" style="width: 20px; height: 20px; " type="checkbox" value="2">
														<span style="padding-left: 10px; font-size: 20px;"> খাল খনন </span>
													</label>
												</div>
											</div>

											<div class="form-group col-md-3">
												<div class="checkbox m-l-xs m-r-xs">
													<label>
														<input id="chkBenificiary3" name="beniandnonbeni" class="grpBeneficiary beniandnonbeni" style="width: 20px; height: 20px; " type="checkbox" value="3">
														<span style="padding-left: 10px; font-size: 20px;"> সেচ সুবিধার উন্নয়ন (সেচ যন্ত্র) </span>
													</label>
												</div>
											</div>

											<div class="form-group col-md-1">
												<div class="m-l-xs m-r-xs bold" style="padding-top: 10px;">
													<label>
														<span style="padding-left: 10px; font-size: 20px;"> | </span>
													</label>
												</div>
											</div>

											<div class="form-group col-md-2">
												<div class="checkbox m-l-xs m-r-xs">
													<label>
														<input id="chkBenificiary4" name="beniandnonbeni" class="grpNonBeneficiary beniandnonbeni" style="width: 20px; height: 20px; " type="checkbox" value="4">
														<span style="padding-left: 10px; font-size: 20px;"> সুবিধাভোগী নয় </span>
													</label>
												</div>
											</div>
										</fieldset>

										<script type="text/javascript">
											$(document).ready(function () {
												$(".grpBeneficiary").change(function () {
													if ($(this).is(':checked')) {
														$(".grpNonBeneficiary").prop('checked', false);
													}
												});

												$(".grpNonBeneficiary").change(function () {
													if ($(this).is(':checked')) {
														$(".grpBeneficiary").prop('checked', false);
													}
												});
											});
										</script>
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-6">
										@Html.LabelFor(model => model.DivisionCode, htmlAttributes: new { @class = "control-label" })
										@Html.DropDownList("DivisionCode", null, string.Empty, new { @id = "ddlDivisionCode", @style = "width: 100%;" })
										@Html.HiddenFor(model => model.DivisionCode)
										@Html.ValidationMessageFor(model => model.DivisionCode, "", new { @class = "text-danger" })
									</div>

									<div class="form-group col-md-6 pull-right">
										@Html.LabelFor(model => model.DistrictCode, htmlAttributes: new { @class = "control-label" })
										<select id="ddlDistrictCode" style="width: 100%;"></select>
										@Html.HiddenFor(model => model.DistrictCode)
										@Html.ValidationMessageFor(model => model.DistrictCode, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-6">
										@Html.LabelFor(model => model.UpazilaCode, htmlAttributes: new { @class = "control-label" })
										<select id="ddlUpazilaCode" style="width: 100%;"></select>
										@Html.HiddenFor(model => model.UpazilaCode)
										@Html.ValidationMessageFor(model => model.UpazilaCode, "", new { @class = "text-danger" })
									</div>

									<div class="form-group col-md-6 pull-right">
										@Html.LabelFor(model => model.UnionCode, htmlAttributes: new { @class = "control-label" })
										<select id="ddlUnionCode" style="width: 100%;"></select>
										@Html.HiddenFor(model => model.UnionCode)
										@Html.ValidationMessageFor(model => model.UnionCode, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-6">
										@Html.LabelFor(model => model.Village, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.Village, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.Village, "", new { @class = "text-danger" })
									</div>

									<div class="form-group col-md-6 pull-right">
										@Html.LabelFor(model => model.Para, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.Para, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.Para, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-6">
										@Html.LabelFor(model => model.WordNo, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.WordNo, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.WordNo, "", new { @class = "text-danger" })
									</div>

									<div class="form-group col-md-6 pull-right">
										@Html.LabelFor(model => model.HouseNo, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.HouseNo, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.HouseNo, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<div class="form-group col-md-6">
										@Html.LabelFor(model => model.NameOfInterviewer, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.NameOfInterviewer, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.NameOfInterviewer, "", new { @class = "text-danger" })
									</div>

									<div class="form-group col-md-6 pull-right">
										@Html.LabelFor(model => model.NameOfSupervisor, htmlAttributes: new { @class = "control-label" })
										@Html.EditorFor(model => model.NameOfSupervisor, new { htmlAttributes = new { @class = "form-control" } })
										@Html.ValidationMessageFor(model => model.NameOfSupervisor, "", new { @class = "text-danger" })
									</div>
								</div>

								<div class="col-md-12">
									<hr />
									<div class="form-group col-md-6">
										<button id="btnSaveMasterInfo" class="btn btn-info"><i class="fa fa-save"></i> Save</button>
										<button id="btnClearMasterInfo" class="btn btn-danger"><i class="fa fa-times"></i> Cancel</button>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>

			<div class="tab-pane fade" id="TabTwo">
				<div class="col-sm-12">
					<div class="panel panel-info">
						<div id="FormHeading" class="panel-heading">
							মডিউল ১নংঃ আর্থ-সামাজিক অবস্থা
						</div>
						<div class="panel-body" style="padding-top: 0px;">
							<div class="row">
								<!--Vertical Tab-->
								<div class="parentVerticalTab">
									<ul class="resp-tabs-list hor_1">
										<li>সেকশন ১</li>
										<li>সেকশন ২</li>
										<li>সেকশন ৩</li>
										<li>সেকশন ৪</li>
										<li>সেকশন ৫</li>
									</ul>
									<div class="resp-tabs-container hor_1">
										<!--section 1-->
										<div class="col-sm-12">
											<div class="panel panel-info">
												<div class="panel-heading">
													সেকশন ১ঃ উত্তরদাতার পরিচিতি ও খানার বর্তমান অবস্থার তথ্যাবলী
												</div>
												<div class="panel-body">
													<div class="col-md-12">
														<div class="form-group pull-right">
															<button class="btn btn-info btnSaveMod1Sec1Detail"><i class="fa fa-save"></i> Save</button>
															<button class="btn btn-danger btnClearMod1Sec1Detail"><i class="fa fa-times"></i> Cancel</button>
														</div>
													</div>

													<div class="col-md-12"><hr /></div>
													<input id="Mod1Sec1DetailId" type="hidden" />

													<div class="col-md-12">
														<div class="form-group col-md-8">
															@Html.Label("NameOfRespondent", "১.১ উত্তরদাতা/ উত্তরদাত্রীর নাম", htmlAttributes: new { @class = "control-label" })
															@Html.Editor("NameOfRespondent", new { htmlAttributes = new { @class = "form-control" } })
														</div>

														<div class="form-group col-md-4 pull-right">
															@Html.Label("MobileNo", "মোবাইল", htmlAttributes: new { @class = "control-label" })
															@Html.Editor("MobileNo", new { htmlAttributes = new { @class = "form-control", @type = "number", @min = 0 } })
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-6">
															@Html.Label("RespondentAge", "১.২ বয়স", htmlAttributes: new { @class = "control-label" })
															@Html.Editor("RespondentAge", new { htmlAttributes = new { @class = "form-control text-right", @type = "number", @min = 0 } })
														</div>
														<div class="form-group col-md-4" style="padding-top: 30px;">
															<label class="control-label">বছর (পূর্ণ বছরে)</label>
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-6">
															@Html.Label("EducationLevelId", "১.৩ শিক্ষাগত যোগ্যতা", htmlAttributes: new { @class = "control-label" })
															@Html.DropDownList("EducationLevelId", new SelectList(ViewBag.EducationLevelId, "EducationLevelId", "EducationLevelName"), string.Empty, new { @class = "form-control" })
														</div>
														<div class="form-group col-md-4" style="padding-top: 30px;">
															<label class="control-label">(সর্বোচ্চ যে শ্রেণী পাস করেছে)</label>
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-6">
															@Html.Label("MaritalStatusId", "১.৪ বৈবাহিক অবস্থা", htmlAttributes: new { @class = "control-label" })
															@Html.DropDownList("MaritalStatusId", new SelectList(ViewBag.MaritalStatusId, "OptionID", "OptionName"), string.Empty, new { @class = "form-control" })
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-6">
															@Html.Label("IsRespondentFamilyMaster", "১.৫ উত্তরদাতা/ উত্তরদাত্রীর পারিবারিক অবস্থান", htmlAttributes: new { @class = "control-label" })
															<select id="IsRespondentFamilyMaster" name="IsRespondentFamilyMaster" class="form-control" style="width: 100%;">
																<option value=""></option>
																<option value="1">1. খানা প্রধান</option>
																<option value="2">2. খানা প্রধান নন</option>
															</select>
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-4">
															@Html.Label("HouseTotalMale", "১.৬ খানার মোট সদস্য সংখ্যা", htmlAttributes: new { @class = "control-label" })
															<input type="number" id="HouseTotalMaleFemale" class="form-control" disabled />
														</div>

														<div class="form-group col-md-4">
															@Html.Label("HouseTotalMale", "পুরুষ", htmlAttributes: new { @class = "control-label" })
															@Html.Editor("HouseTotalMale", new { htmlAttributes = new { @class = "form-control text-right", @type = "number", @min = 0 } })
														</div>

														<div class="form-group col-md-4 pull-right">
															@Html.Label("HouseTotalFemale", "মহিলা", htmlAttributes: new { @class = "control-label" })
															@Html.Editor("HouseTotalFemale", new { htmlAttributes = new { @class = "form-control text-right", @type = "number", @min = 0 } })
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-6">
															@Html.Label("IsAgriMainOccupation", "১.৭ কৃষিকাজ খানার কোন ধরনের পেশা", htmlAttributes: new { @class = "control-label" })
															<select id="IsAgriMainOccupation" name="IsAgriMainOccupation" class="form-control" style="width: 100%;">
																<option value=""></option>
																<option value="1">1. প্রধান</option>
																<option value="2">2. অপ্রধান</option>
															</select>
														</div>
													</div>

													<div class="col-md-12">
														<div class="form-group col-md-4">
															@Html.Label("HouseTotalMale", "১.৮ খানার কতজন কৃষি খাতের সাথে জড়িত", htmlAttributes: new { @class = "control-label" })
															<input type="number" id="HouseAgriTotalMaleFemale" class="form-control" disabled />
														</div>

														<div class="form-group col-md-4">
															@Html.Label("HouseAgriTotalMale", "পুরুষ", htmlAttributes: new { @class = "control-label" })
															@Html.Editor("HouseAgriTotalMale", new { htmlAttributes = new { @class = "form-control text-right", @type = "number", @min = 0 } })
														</div>

